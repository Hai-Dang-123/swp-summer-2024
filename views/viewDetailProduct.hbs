                    <p>Year: {{ product.yearOfProduction }}</p>
                </div>
                <div class="product-price">
                    ${{ product.price }}
                </div>
                <!--add to cart-->
                <div class="product-action">
                    <button id="addToCartBtn">Add to Basket</button>
                </div>

                <script>
                    // Hàm để lấy giá trị của query parameter từ URL
                    function getQueryParam(name) {
                        const urlParams = new URLSearchParams(window.location.search);
                        return urlParams.get(name);
                    }

                    // Kiểm tra nếu có thông báo từ URL
                    const message = getQueryParam('message');
                    if (message) {
                        // Nếu có, hiển thị thông báo ra màn hình
                        alert(message);
                    }
                </script>

                <script>
                    document.getElementById('addToCartBtn').addEventListener('click', addToCart);

                    async function addToCart() {
                        const productId = product.id; // Thay thế 'YOUR_PRODUCT_ID' bằng ID của sản phẩm bạn muốn thêm vào giỏ hàng
                        const quantity = 1; // Số lượng sản phẩm bạn muốn thêm vào giỏ hàng

                        try {
                            const response = await fetch(`/cart/add-to-cart/${productId}`, {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({ quantity })
                            });

                            if (response.ok) {

                                window.location.reload();
                            } else {

                                console.error('Failed to add product to cart:', response.statusText);

                            }
                        } catch (error) {
                            console.error('Failed to add product to cart:', error.message);

                        }
                    }
                </script>
                <!--end add to cart-->


                <div class="product-info">
                    <p><strong>Delivery:</strong> Free delivery within 1-2 business days</p>
                    <p><strong>Returns:</strong> 14-day returns for your peace of mind</p>
                    <p><strong>Authenticity:</strong> Guaranteed authentic by Watchfinder</p>
                    <p><strong>Warranty:</strong> Watchfinder 24-month warranty</p>
                </div>
            </div>
        </div>
    </div>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Lấy tham chiếu của nút "Add to Basket"
            var addToCartBtn = document.getElementById("addToCartBtn");

            // Thêm sự kiện click vào nút
            addToCartBtn.addEventListener("click", function () {
                // Gửi yêu cầu POST khi người dùng nhấp vào nút
                fetch('/cart/add', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        // Thêm dữ liệu sản phẩm cần thêm vào giỏ hàng ở đây nếu cần
                    })
                })
                    .then(response => {
                        // Kiểm tra nếu yêu cầu được xử lý thành công
                        if (response.ok) {
                            // Chuyển hướng người dùng trở lại trang viewProduct
                            window.location.href = '/viewProduct';
                        } else {
                            // Xử lý lỗi nếu cần
                            console.error('Failed to add item to cart');
                        }
                    })
                    .catch(error => {
                        console.error('Error adding item to cart:', error);
                    });
            });
        });

        //--------------- xử lý icon
        // Đợi tất cả các tài liệu được tải trước khi thực hiện mã JavaScript
        document.addEventListener("DOMContentLoaded", function () {
            // Lấy tham chiếu của thẻ span hiển thị số lượng sản phẩm trong giỏ hàng
            var cartItemCount = document.getElementById("cartItemCount");

            // Gửi yêu cầu GET để lấy thông tin giỏ hàng từ máy chủ
            fetch('/cart')
                .then(response => response.json())
                .then(data => {
                    // Tính tổng số lượng sản phẩm trong giỏ hàng
                    var totalItems = data.reduce((acc, curr) => acc + curr.quantity, 0);
                    // Cập nhật số lượng sản phẩm trong giỏ hàng trên icon
                    cartItemCount.textContent = totalItems;
                })
                .catch(error => {
                    console.error('Error fetching cart data:', error);
                });
        });


    </script>



